{
  "name": "Instagram Voice Downloader",
  "description": "Download voice messages from Instagram Web.",
  "version": "1.2",
  "manifest_version": 2,
  "browser_action": {
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "main.js"
      ],
      "matches": [
        "https://www.instagram.com/direct/*"
      ]
    }
  ]
}